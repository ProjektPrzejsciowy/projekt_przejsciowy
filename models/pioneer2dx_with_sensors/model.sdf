    <!-- IMU -->
    <include>
      <uri>model://imu</uri>
      <pose>0.0 0 0.28 0 0 0</pose>
    </include>
    <joint name="imu_joint" type="revolute">
      <child>imu::link</child>
      <parent>chassis</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <upper>0</upper>
          <lower>0</lower>
        </limit>
      </axis>
    </joint>
    
